{
  "publisher": "LemonNeko",
  "name": "vscode-transform",
  "displayName": "VSCode Transform",
  "version": "0.1.0",
  "description": "A converter collection in your vscode.",
  "repository": {
    "type": "git",
    "url": "https://github.com/LemonNekoGH/vscode-transform"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.91.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vscode-transform.json-to-go",
        "category": "VSCode Transform",
        "title": "JSON to Go struct"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "type-check": "tsc --noEmit",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run build:test && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "compile": "npm run type-check",
    "publish": "vsce publish",
    "package": "vsce package"
  },
  "dependencies": {
    "@lemonneko/gofmt.js": "^0.3.0",
    "@types/json-to-go": "^1.0.0",
    "json-to-go": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.12.2",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "@vscode/vsce": "^2.31.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
